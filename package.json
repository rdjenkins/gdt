{
  "name": "grampound-digital-twin",
  "private": true,
  "version": "0.9.5",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && npm update && vite build",
    "android": "tsc && npm update && vite build && npx cap sync && npx cap run android",
    "iosSPMfix": "sed -i '' 's/.iOS(.v18)/.iOS(\"18.0\")/g' ios/App/CapApp-SPM/Package.swift",
    "crossbuild": "tsc && npm update && vite build && npx cap sync && npm run iosSPMfix",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
    "typescript": "~5.8.3",
    "vite": "^7.1.2"
  },
  "dependencies": {
    "@capacitor-community/fcm": "^8.1.0",
    "@capacitor/android": "^8.0.0",
    "@capacitor/cli": "^8.0.0",
    "@capacitor/core": "^8.0.0",
    "@capacitor/filesystem": "^8.0.0",
    "@capacitor/haptics": "^8.0.0",
    "@capacitor/ios": "^8.0.0",
    "@capacitor/local-notifications": "^8.0.0",
    "@capacitor/push-notifications": "^8.0.0",
    "@capacitor/toast": "^8.0.0",
    "@ionic/pwa-elements": "^3.3.0",
    "openmeteo": "^1.2.1",
    "ts-md5": "^2.0.1"
  }
